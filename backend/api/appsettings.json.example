{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "server=YOUR_SERVER;database=YOUR_DATABASE;user=YOUR_USER;password=YOUR_PASSWORD;"
    },
    "JWT": {
        "Key": "YOUR_SECRET_KEY",
        "Issuer": "YOUR_ISSUER",
        "Audience": "YOUR_AUDIENCE",
        "ExpireMonths": "YOUR_EXPIRE_MONTHS"
    }
}
